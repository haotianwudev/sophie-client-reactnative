{
  "expo": {
    "name": "SOPHIE",
    "slug": "sophie-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/agents/SOPHIE.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/agents/SOPHIE.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sophieai.client"
    },
    "android": {
      "privacyPolicy": "https://sophieai.dev/privacy-policy.html",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/agents/SOPHIE.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sophieai.client",
      "versionCode": 1,
      "permissions": [],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.sophieai.dev",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "androidNavigationBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content",
      "translucent": false
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "sophie-client"
      }
    }
  }
}
